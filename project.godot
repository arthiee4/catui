config_version=5

[animation]

compatibility/default_parent_skeleton_in_mesh_instance_3d=true

[application]

config/name="catui"
run/main_scene="uid://bv56lsy0gne0m"
config/features=PackedStringArray("4.6", "C#", "Forward Plus")
run/max_fps=60
run/low_processor_mode=true
boot_splash/bg_color=Color(1, 1, 1, 1)
boot_splash/show_image=false
config/icon="uid://dwlfwa53ga11a"

[autoload]

LibretroPlayer="*res://src/emu/libretro/LibretroPlayer.cs"
GlobalAutoload="*res://src/autoload/GlobalAutoload.gd"
EmulatorConfig="*res://src/autoload/EmulatorConfig.gd"
AutoSaveManager="*res://src/autoload/AutoSaveManager.gd"
SFX="*res://src/autoload/SFX.gd"
ButtonLayoutManager="*res://src/autoload/ButtonLayoutManager.gd"
DebugCapture="*res://src/autoload/DebugCapture.gd"
AndroidManager="*res://src/autoload/AndroidAppManager.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/resizable=false
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="catui"

[editor_plugins]

enabled=PackedStringArray("res://addons/CatUILauncher_Plugin/plugin.cfg")

[file_customization]

folder_colors={
"res://assets/": "red"
}

